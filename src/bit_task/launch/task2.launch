<launch>

  <!--bringup robot-->  
  <include file="$(find bit_task)/launch/slam_map.launch" />


  <!--运行模型显示节点-->
  <include file="$(find bit_robotcar)/launch/view_ur5e_challenge2.launch" />

  <!--bringup zed-->  
  <include file="$(find zed_wrapper)/launch/zedm.launch" />
  <include file="$(find bit_hardware_interface)/launch/CameraMER.launch" />
  <!--运行图像hdr处理节点-->
  <node pkg="bit_hardware_interface" type="hdr_generate.py" name="gen_hdrImage" respawn="true" />
  <!--运行视觉算法节点-->
  <node pkg="bit_vision" type="task2VisionPrc" name="task2VisionPrc" respawn="true" />
  <node pkg="bit_vision" type="locate_precise_MER" name="MERVisionPrc" respawn="true" />
  
  <!--运行协同规划节点
  <node pkg="bit_plan" type="task2" name="task2" respawn="false" />-->

  <!--运行任务节点-->
  <!--<node pkg="bit_task" type="task2_action_test_know" name="task2_action" output="screen" respawn="true" />-->

  <!--运行取放砖运动节点-->
  <node pkg="bit_motion" type="t2_pick_put_server.py" name="pickputAction" output="screen" respawn="true" />

  <!--运行取放砖运动节点-->
  <node pkg="bit_task" type="map_address_manage" name="address_manage_node" output="screen" respawn="true" />

  <!--运行找砖位置节点-->
  <!--<node pkg="bit_motion" type="t2_locate_server.py" name="locate_server_node" respawn="false" />-->

  

</launch>
